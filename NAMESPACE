useDynLib(ma, .registration = TRUE)
import(stats, doParallel, foreach, parallel, quadprog)
importFrom("graphics", "lines", "par", "plot", "points", "rug", "abline")
importFrom("grDevices", "n2mfrow")
export(lm.ma)

S3method(lm.ma, formula)
S3method(lm.ma, default)
S3method(predict, lm.ma)
S3method(print, lm.ma)
S3method(summary, lm.ma)
S3method(coef, lm.ma)
S3method(plot, lm.ma)
